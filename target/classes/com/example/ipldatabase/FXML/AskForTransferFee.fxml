               textFill="#dfe4ea">
            <font>
                <Font name="Bookman Old Style Bold" size="18.0"/>
            </font>
        </Label>
        <Label fx:id="estimatedValue" alignment="CENTER" layoutY="86.0" prefHeight="35.0" prefWidth="483.0" text="Label"
